<%- include("partials/header.ejs") %>
<div class="cont">
  <div id="updateForm" class="registration-form">
    <form id="bookForm" action="/submit" method="post">
      <input type="text" name="name" placeholder="name" required>
      <input type="text" name="info1" placeholder="info1" required>
      <input type="text" name="info2" placeholder="info2" required>
      <input type="text" name="info3" placeholder="info3" required>
      <select id="category" name="category" required>
        <option value="Books">Books</option>
        <option value="Fortune">Fortune</option>
        <option value="Food">Food</option>
        <option value="Quotes">Quotes</option>
        <option value="Anime">Anime</option>
        <option value="Movies">Movies</option>
        <option value="Travel">Travel</option>
      </select>
      <textarea id="description" name="description" rows="4" cols="50" placeholder="Enter Overview here..." required></textarea>
      <button type="submit">Submit</button>
    </form>
  </div>
</div>
